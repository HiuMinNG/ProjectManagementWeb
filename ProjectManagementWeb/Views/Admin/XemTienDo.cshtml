@model IEnumerable<ProjectManagementWeb.Models.DuAn>

@{
    ViewBag.Title = "XemTienDo";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2 style="text-align:center ; ">Xem Tiến Độ</h2>


<table class="table" >
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DuAnID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MoTa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayBatDau)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayKetThuc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NganSach)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DuAnID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MoTa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayBatDau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayKetThuc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NganSach)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit",null  ,new { id = item.DuAnID}, new { @class = "btn btn-outline-primary" }) |
            @Html.ActionLink("Details", "Details",null, new { id = item.DuAnID }, new { @class = "btn btn-outline-success" }) |
            @Html.ActionLink("Delete", "Delete",null , new { id = item.DuAnID }, new { @class = "btn btn-outline-danger" })
        </td>
    </tr>
}

</table>
